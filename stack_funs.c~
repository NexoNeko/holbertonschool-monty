#include "main.h"
/**
 * stack_add - adds a new node to the begining of the stack
 *
 * @head: List to add a node to
 * @n: Value to store in the new node
 *
 * Return: Pointer to last node
 */
dlistint_t *stack_add(dlistint_t **head, const int n)
{
	dlistint_t *nuNode = malloc(sizeof(dlistint_t));

	if (!nuNode || !head)
		free(nuNode);
	else
	{
		if (*head)
			(*head)->prev = nuNode;
		nuNode->n = n;
		nuNode->next = (*head);
		(*head) = nuNode;
	}
	return (nuNode);
}
